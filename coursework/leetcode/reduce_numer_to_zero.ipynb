   "source": [
    "class Solution:\n",
    "    def numberOfSteps (self, num: int) -> int:\n",
    "        i = 0\n",
    "        while num != 0:\n",
    "            num = num /2 if num % 2 == 0 else num -1\n",
    "            i += 1\n",
    "        return i\n",
    "        "
   ]
